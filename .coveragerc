[run]
source = app
omit = 
    */tests/*
    */venv/*
    */.venv/*
    */migrations/*
    app/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    @abstract

[html]
directory = htmlcov
